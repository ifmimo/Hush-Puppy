<Event>
<Name>On Menu</Name>
<Args>number e_ID, table e_ItemInfo</Args>
<Script>-- File Menu
	
	---- Options/Settings
	if e_ID == 101 then
		DialogEx.Show("Options")
	end
	
	---- Close Software From Menu
	if e_ID == 102 then
		Application.Exit(0);
	end
	
-- Add Menu

	---- New Entry
	if e_ID == 201 then
		HP:Add();
	end
	
	---- New Servcie
	if e_ID == 202 then
		IconUpdated = DialogEx.Show("Servcie", true);
		if IconUpdated == 9 then
			HP:Clear()
			HP:BuildIconCashe();
			HP:PullServices();
		end
	end
	
-- View Menu

	---- View Entries
	if e_ID == 301 then
		HP:PullServices();
	end
	---- View Services
	if e_ID == 302 then
		HP:Add();
	end
	
-- About

	---- About Rexzooly
	if e_ID == 401 then
		HP:About('r')
	end
	---- About AMS
	if e_ID == 402 then
		HP:About('a')
	end
	---- About HP
	if e_ID == 403 then
		HP:About('h')
	end</Script>
