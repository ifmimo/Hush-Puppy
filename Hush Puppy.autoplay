HP = {
		-- Edit this below to what ever you would like the title to be EG: Title = 'My Software 01';
		Title = 'Hush Puppy (0.3) By Rexzooly Kai Black', 
		-- This is the name of the database file you can name this anything EG: db.msc
		DatabaseName = 'core.sys'
	};


-- Since we have already set this in the main setup section we don't need to edit this path.
-- WARNING EDIT ONLY IF YOU KNOW WHAT YOUR DOING
HP.Root = Shell.GetFolder(SHF_MYDOCUMENTS).."\\"..HP.DatabaseName;



-- These two functions are needed to ecode and decode the details within the database but you can create your own if you like, this was made just for basic encoding
-- #1
function HP:Encode(s_string)
	HPTool = System.GetLANInfo();
	LockedString = Crypto.BlowfishEncryptString(s_string, HPTool.Host..HPTool.User, 0);
	code = string.reverse(LockedString);
	return enc(code);
end
--#2
function HP:Decode(s_string)
	HPTool = System.GetLANInfo();
	code = dec(s_string);
	LockedString = string.reverse(code);
	CleanString = Crypto.BlowfishDecryptString(LockedString, HPTool.Host..HPTool.User);
	return CleanString;
end



-- This function creats the database, requests the user password and sets up the app to be read to work, 
-- WARNING EDIT ONLY IF YOU KNOW WHAT YOUR DOING
function HP:Create(b_locked)
	Window.SetText(Application.GetWndHandle(), HP.Title);
	if b_locked == nil or b_locked == '' then
		b_locked = true;
	else
		if type(b_locked) == 'boolean' then
			b_locked = b_locked;
		else
			if type(b_locked) == 'string' then
				if b_locked == 'true' then
					b_locked = true;
				end
				if b_locked == 'false' then
					b_locked = false;
				end
				if b_locked ~= 'false' and b_locked ~= 'true' then
					b_locked = true;
				end
			else
				b_locked = true;
			end
		end
	end
	if not File.DoesExist(HP.Root) then
		TextFile.WriteFromString(HP.Root, "", false);
		SQLite3CreateConnection, err = SQLite3:connect(HP.Root);
		if SQLite3CreateConnection then
			returns, err = SQLite3CreateConnection:execute("CREATE TABLE bank (ID INTEGER PRIMARY KEY, icon INTEGER, sname VARCHAR(300), email VARCHAR(300), auth VARCHAR(300))");
			if err then
				Dialog.Message(HP.Title, err);
			end
			returns, err = SQLite3CreateConnection:execute("CREATE TABLE settings (ID INTEGER PRIMARY KEY, mode VARCHAR(300), auth VARCHAR(300))");
			if err then
				Dialog.Message(HP.Title, err);
			end
			SQLite3CreateConnection:close();
			PassMe = Dialog.Message(HP.Title, 'Would you like to enable password mode? This will lock the software with your own password and request it when you open the program.', MB_YESNO, MB_ICONNONE, MB_DEFBUTTON2);
			if b_locked then
				if PassMe == IDYES then
					PinAurth = Dialog.PasswordInput('Please enter your new pin', 'If you leave this blank the app will default your pin to 0000', MB_ICONNONE);
					if PinAurth == nil or PinAurth == '' then
						HP:SetUser('1', '0000');
					else
						HP:SetUser('1', PinAurth);
					end
				else
					HP:SetUser('0', '0000');
				end
			end
		else
			Dialog.Message(HP.Title.." Error", err);
		end
	else
		if b_locked then 
			HP:PullUser();
		end
	end
end

-- This function will drop a given Entry
function HP:DropEntry(s_id)
	SQLite3DropConnection, err = SQLite3:connect(HP.Root);
	if not SQLite3DropConnection and err then
		Dialog.Message(HP.Title.." Error", err);
	else
		returns, err = SQLite3DropConnection:execute(string.format("DELETE FROM bank WHERE ID = '%s'", s_id));				
		if err then
			Dialog.Message(HP.Title, err);
		end
	end
	SQLite3DropConnection:close();
end

function HP:SetUser(Mode, Auth)
	SQLite3SetUserConnection, err = SQLite3:connect(HP.Root);
	if not SQLite3SetUserConnection and err then
		Dialog.Message(HP.Title.." Error", err);
	else
		returns, err = SQLite3SetUserConnection:execute(string.format("INSERT INTO settings (mode, auth) VALUES('%s', '%s')", Mode, HP:Encode(Auth)));
		if err then
			Dialog.Message(HP.Title, err);
		end
	end
	SQLite3SetUserConnection:close();
end

function HP:DeleteUser()
	SQLite3UserConnection, err = SQLite3:connect(HP.Root);
	if not SQLite3Connection and err then
		Dialog.Message(HP.Title.." Error", err);
	else
		returns, err = SQLite3UserConnection:execute(string.format("UPDATE settings SET mode = '%s'", '0'));
		if err then
			Dialog.Message(HP.Title, err);
		end
	end
	SQLite3UserConnection:close();
end

function HP:PullUser()
	SQLite3PullUserConnection, err = SQLite3:connect(HP.Root);
	if not SQLite3PullUserConnection and err then
	    Dialog.Message(HP.Title.." Error", err);
	else
		SQLite3PullUserCursor, err = SQLite3PullUserConnection:execute("SELECT * FROM settings");
		if err then
			Dialog.Message(HP.Title, err);
		end
		row = SQLite3PullUserCursor:fetch({}, 'a');
		while row do
			if row.mode == '1' then
				wrong = true
				CountWrong = 0;
				Trys = 3;
				while wrong do
					thispin = Dialog.PasswordInput('Enter Pin', 'Please enter pin, you have '..Trys..' left', MB_ICONNONE);
					if HP:Encode(thispin) ~= row.auth then
						CountWrong = CountWrong+1;
						Trys = Trys -1;
						if CountWrong &gt;= 3 then
							Dialog.Message(HP.Title, 'Wrong Pin, Now Closing');
							Application.Exit(0);
						end
					else
						wrong = false;
						break;
					end
				end
				break;
			else
				row = SQLite3PullUserCursor:fetch(row,"a");
			end
		end
	end
	SQLite3PullUserCursor:close();
	SQLite3PullUserConnection:close();
end

function HP:AddServices(s_service, s_email, s_auth)
	if not File.DoesExist(HP.Root) then
		Dialog.Message(HP.Title, '404, Missing Database');
	else
		SQLite3AddConnection, err = SQLite3:connect(HP.Root);
		if not SQLite3AddConnection and err then
		    Dialog.Message(HP.Title, err);
		else
			s_auth = HP:Encode(s_auth);
			SQLite3AddConnection:execute("INSERT INTO bank (icon, sname, email, auth) VALUES ('1', '"..s_service.."', '"..s_email.."', '"..s_auth.."')");
			SQLite3AddConnection:close();
		end
	end
end

function HP:PullServices()
	HPMode = "Show";
	ListIcon.ClearRows("ListBox1");
	ListIcon.SetView("ListBox1", LISTICON_DETAIL);
	SQLite3Connection, err = SQLite3:connect(HP.Root);
	if not SQLite3Connection and err then
	    Dialog.Message(HP.Title.." Error", err);
	else
		SQLite3Cursor = SQLite3Connection:execute("SELECT * FROM bank ORDER BY LOWER(sname), sname ASC");
		row = SQLite3Cursor:fetch({},"a");
		while row do
			ShowService = {ID = string.format("%s", row.ID), Icon = string.format("%s", row.icon), Name = string.format("%s", row.sname), Email = string.format("%s", row.email), Auth = HP:Decode(row.auth)}; 
	   		ListIcon.AddRow("ListBox1", ShowService.Name..NEXT_CELL..ShowService.Email..NEXT_CELL..ShowService.Auth..NEXT_CELL..ShowService.ID, HP:ServiceIcon(ShowService.Name));
		    row = SQLite3Cursor:fetch(row,"a");
		end
		SQLite3Cursor:close();
		SQLite3Connection:close();
	end
end

function HP:ServiceIcon(s_string)
	if ServiceIcon[s_string] == "" or ServiceIcon[s_string] == nil or ServiceIcon[s_string] == "Custom" then
		return ServiceIcon["Default"];
	else
		return ServiceIcon[s_string];
	end
end

function HP:BuildIconCashe()
	local Folders = Folder.Find(_SourceFolder.."\\Services\\", "*", false, nil);
	ServiceIcon = {};
	if (Folders) then
		for K, V in pairs(Folders) do
			if (File.DoesExist(V.."\\icon.png")) then
				ServiceIcon[String.SplitPath(V).Filename] = ListIcon.LoadImage(6, V.."\\icon.png")
				ListIcon.AddRow("ListBox1", String.SplitPath(V).Filename, ServiceIcon[String.SplitPath(V).Filename]);
			end
		end
	end
	ServiceIcon["Default"] = ListIcon.LoadImage(6, _SourceFolder.."\\AutoPlay\\Images\\1303256395_license-key.png"); 
end

function HP:Clear()
	local Folders = Folder.Find(_SourceFolder.."\\Services\\", "*", false, nil);
	if (Folders) then
		for K, V in pairs(Folders) do
			if (File.DoesExist(V.."\\icon.png")) then
				ListIcon.FreeImage(ServiceIcon[String.SplitPath(V).Filename]);
			end
		end
	end
	ListIcon.FreeImage(ServiceIcon["Default"]);
end

function HP:Add()
	HPMode = "Add";
	ListIcon.ClearRows("ListBox1");
	ListIcon.SetView("ListBox1", 0);
	local Folders = Folder.Find(_SourceFolder.."\\Services\\", "*", false, nil);
	if (Folders) then
		for K, V in pairs(Folders) do
			if (File.DoesExist(V.."\\icon.png")) then
				ListIcon.AddRow("ListBox1", String.SplitPath(V).Filename, ServiceIcon[String.SplitPath(V).Filename]);
			end
		end
	end
	ListIcon.AddRow("ListBox1", "Custom", ServiceIcon["Default"]);
end

function HP:AddIcon(s_name, s_path)
	ThisService = Folder.DoesExist(_SourceFolder.."\\Services\\"..s_name);
	if not ThisService then
		Folder.Create(_SourceFolder.."\\Services\\"..s_name);
		File.Copy(s_path, _SourceFolder.."\\Services\\"..s_name.."\\icon.png", false, false, true, false);	
		return true;
	else
		return false;
	end
end

