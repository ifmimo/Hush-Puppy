<Args/>
<Script>if ToAdd.Name == "Custom" then
	BeforeAdd = Dialog.Input("Custom Service Name", "Please enter a custom name below", "", MB_ICONNONE);
	if BeforeAdd ~= "CANCEL" and BeforeAdd ~= "" then
		ToAdd.Name = BeforeAdd;
		Image.Load("Service_Icon", ToAdd.Path);
		Label.SetText("Service_Label", ToAdd.Name);
	else
		Dialog.Message("Hush Puppy", "You must input your service name");
		BeforeAdd = Dialog.Input("Custom Service Name", "Please enter a custom name below", "", MB_ICONNONE);
		if BeforeAdd ~= "CANCEL" and BeforeAdd ~= "" then
			ToAdd.Name = BeforeAdd;
			Image.Load("Service_Icon", ToAdd.Path);
			Label.SetText("Service_Label", ToAdd.Name);
		else
			Dialog.Message("Hush Puppy", "You failed to add a service name, action exited");
			DialogEx.Close(0);
		end
	end	
else
	Image.Load("Service_Icon", ToAdd.Path);
	Label.SetText("Service_Label", ToAdd.Name);
end</Script>
