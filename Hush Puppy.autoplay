function ListIconEvent.OnRightClick(ObjectName, Row)
	if HPMode == 'Add' then
		local Position, Menu = System.GetMousePosition(false), {};
		Menu[1] = {Enabled = false, Text = 'Add New Service', ID = 1};
		if (Row and Row ~= -1) then
			if ToDelete.Services ~= 'Custom' then
				Menu[2] = {Text = '---'};
				Menu[3] = {Enabled = false, Text = 'Remove '..ToDelete.Services, ID = 2};
				Menu[4] = {Enabled = false, Text = 'Remove Entries for '..ToDelete.Services, ID = 3};
			end
		end
		local nRes = Application.ShowPopupMenu(Position.X, Position.Y, Menu, 0, 0, true, false);
		 
		if(nRes ~= -1)then
			if (nRes == 1) then
				Dialog.Message(HP.Title, 'Coming Soon, like one said.', MB_OK, MB_ICONNONE);
			end
		end
	end
	if HPMode == 'Show' then
		if (Row and Row ~= -1) then
			local Position, Menu = System.GetMousePosition(false), {};
			Menu[1] = {Enabled = false, Text = ToDelete.Menu, ID = 1};
			Menu[2] = {Text = "---"};
			Menu[3] = {Text = "Copy username to Clipboard..", ID = 2};
			Menu[4] = {Text = "Copy password to Clipboard..", ID = 3};
			Menu[5] = {Text = "Delete "..ToDelete.Menu, ID = 4};
			Menu[6] = {Enabled = false, Text = "Update "..ToDelete.Menu, ID = 5};
			
			local nRes = Application.ShowPopupMenu(Position.X, Position.Y, Menu, 0, 0, true, false);
			
			if (nRes ~= -1) then
				if (nRes == 1) then
				elseif (nRes == 2) then
					Clipboard.CopyText(ListIcon.GetCellText(ObjectName, Row, 1));
					if (Application.GetLastError() ~= 0) then
						return Dialog.Message("Error", "Failed to copy username to Clipboard.\r\n\r\n".._tblErrorMessages[Application.GetLastError()], 0, 16, 0);
					else
						return Dialog.Message("Success", "Successfully copied username to Clipboard!", 0, 64, 0);
					end
				elseif (nRes == 3) then
					Clipboard.CopyText(ListIcon.GetCellText(ObjectName, Row, 2));
					if (Application.GetLastError() ~= 0) then
						return Dialog.Message("Error", "Failed to copy password to Clipboard.\r\n\r\n".._tblErrorMessages[Application.GetLastError()], 0, 16, 0);
					else
						return Dialog.Message("Success", "Successfully copied password to Clipboard!", 0, 64, 0);
					end
				elseif (nRes == 4) then
					local ToDelete = {}; 
					ToDelete.ID = ListIcon.GetCellText(ObjectName, Row, 3);
					ToDelete.Services = ListIcon.GetCellText(ObjectName, Row, 0);
					ToDelete.Username = ListIcon.GetCellText(ObjectName, Row, 1);
					ToDelete.Menu = ToDelete.Services..' ('..ToDelete.Username..')';
					local MessageDelete =[[Are you sure you want to delete the following?
					Entry: #]]..ToDelete.ID..[[,				
					Username: ]]..ToDelete.Username..[[,
					Service: ]]..ToDelete.Services..[[.]];
					local DeleteMe = Dialog.Message("Delete", MessageDelete, MB_OKCANCEL, MB_ICONNONE, MB_DEFBUTTON2);	
					if DeleteMe == IDOK then
						HP:DropServices(tonumber(ToDelete.ID));
						HP:PullServices();
					end
				end
			end
		end
	end
end

